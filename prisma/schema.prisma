generator client {
  provider = "prisma-client-js"
  output = "../../node_modules/.prisma/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Configuration {
  id              String  @id @default(cuid())
  width           Int
  height          Int
  imageUrl        String
  croppedImageUrl String?
}
