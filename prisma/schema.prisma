generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Configuration {
  id              String        @id @default(cuid())
  width           Int
  height          Int
  imageUrl        String
  croppedImageUrl String?
}

model User {
  id              String        @id @default(cuid())
  email           String
  createdAt       DateTime      @default(now())
  updatedAt       DateTime      @updatedAt
}